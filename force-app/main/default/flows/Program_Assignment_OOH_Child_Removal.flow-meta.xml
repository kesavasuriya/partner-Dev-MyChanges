    <decisions>
        <name>Check_Relationship_values</name>
        <label>Check Relationship values</label>
        <locationX>660</locationX>
        <locationY>638</locationY>
        <defaultConnector>
            <targetReference>Iterate_through_relationship_records</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>If_Parent</name>
            <conditionLogic>(1 AND 3) OR (2 AND 3) OR 4</conditionLogic>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Relationship_Values__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Father - Biological</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Relationship_Values__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Mother - Biological</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Person_Related_To__r.Age__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>21.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Is_Caregiver__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Parent_Guardian_of_Child_in_OOH</targetReference>
            </connector>
            <label>If Parent</label>
        </rules>
        <rules>
            <name>If_Sibling</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Relationship_Values__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Brother - Biological</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Relationship_Values__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sister - Biological</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Sibling_of_Child_in_OOH</targetReference>
            </connector>
            <label>If Sibling</label>
        </rules>
        <rules>
            <name>If_minor_parent</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Relationship_Values__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Father - Biological</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Relationship_Values__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Mother - Biological</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Iterate_through_relationship_records.Person_Related_To__r.Age__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>21.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Child_of_a_Minor_Parent_in_OOH</targetReference>
            </connector>
            <label>If minor parent</label>
        </rules>
    </decisions>
