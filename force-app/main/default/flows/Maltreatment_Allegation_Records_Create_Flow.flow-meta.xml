    </formulas>
    <formulas>
        <name>MENTAL_INJURY_ABUSE_Formula</name>
        <dataType>String</dataType>
        <expression>IF({!Get_SDM_Records.Abuse_child_has_an_observable_identifi__c},&quot;MENTAL INJURY - ABUSE&quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
