        <dataType>String</dataType>
        <expression>IF({!Get_SDM_Records.Neglect_child_has_an_observable_identif__c},&quot;MENTAL INJURY - NEGLECT&quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>Neglect_Formula</name>
        <dataType>String</dataType>
        <expression>IF(OR({!Get_SDM_Records.Suspicious_death_of_child_due_to_neglect__c}, {!Get_SDM_Records.signs_or_diagnosis_of_non_organic__c}, {!Get_SDM_Records.Inadequate_of_food_nutrition__c}, {!Get_SDM_Records.A_child_discharged_from_a_facility__c}, {!Get_SDM_Records.Exposure_to_unsafe__c}, {!Get_SDM_Records.Inadequate_clothing_or_hygiene__c}, {!Get_SDM_Records.Inadequate_supervision__c}, {!Get_SDM_Records.Physical_treatment_of_the_child_poses__c}) ,&quot;Neglect&quot; ,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>Physical_Abuse_Formula</name>
        <dataType>String</dataType>
        <expression>IF(OR({!Get_SDM_Records.Suspicious_death_of_a_child_due_to_abuse__c}, {!Get_SDM_Records.Non_accidental_physical_injury__c}, {!Get_SDM_Records.Injury_inconsistent_with_explanation__c}, {!Get_SDM_Records.Injury_that_appears_suspicious__c}, {!Get_SDM_Records.Giving_child_toxic_chemicals_alcohol__c}, {!Get_SDM_Records.Caregiver_action_that_caused_injury__c}),&quot;PHYSICAL ABUSE&quot; ,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>RISK_OF_HARM_Formula</name>
        <dataType>String</dataType>
        <expression>IF(OR({!Get_SDM_Records.Prior_child_fatality_and_a_new_child__c}, {!Get_SDM_Records.Substance_Exposed_Newborn__c}, {!Get_SDM_Records.Child_s_basic_need_to_be_unmet__c}, {!Get_SDM_Records.Risk_of_sex_Abuse__c}, {!Get_SDM_Records.Risk_of_Domestic_Violence__c}, {!Get_SDM_Records.Harm_Risk_of_Sex_Trafficking__c}, {!Get_SDM_Records.Previous_Fatality_or_Serious_Injury__c}, {!Get_SDM_Records.Prior_Indicated_or_Unsub_Finding__c}, {!Get_SDM_Records.Adult_Survivor_of_child_Maltreatment__c}, {!Get_SDM_Records.Birth_Match__c}, {!Get_SDM_Records.Allegations_that_the_maltreatment__c}),&quot;RISK OF HARM&quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>Selected_Abuse_Values</name>
        <dataType>String</dataType>
        <expression>{!ABANDONMENT_Formula}+{!Failure_To_Protect_Formula}+{!MEDICAL_NEGLECT_Formula}+{!MENTAL_INJURY_ABUSE_Formula}+{!MENTAL_INJURY_NEGLECT_Formula}+{!Neglect_Formula}+{!Physical_Abuse_Formula}+{!RISK_OF_HARM_Formula}+{!Sexual_Abuse_Formula}+{!UNATTENDED_CHILD_Formula}</expression>
    </formulas>
    <formulas>
        <name>Sexual_Abuse_Formula</name>
        <dataType>String</dataType>
        <expression>IF(OR({!Get_SDM_Records.Sexual_molestation_of_a_child__c}, {!Get_SDM_Records.Sexual_act_s_among_siblings_or_other__c}, {!Get_SDM_Records.Sexual_exploitation_of_a_child__c}, {!Get_SDM_Records.Physical_behavioral_or_suspicious__c}, {!Get_SDM_Records.Risk_of_Sex_Trafficking__c}) , &quot;SEXUAL ABUSE&quot; ,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>UNATTENDED_CHILD_Formula</name>
        <dataType>String</dataType>
        <expression>IF(OR({!Get_SDM_Records.A_child_of_any_age_who_is_physically__c}, {!Get_SDM_Records.A_child_under_8_has_been_left_alone__c}, {!Get_SDM_Records.A_child_over_the_age_of_8_has_been_left__c}),&quot;UNATTENDED CHILD&quot;,&quot;&quot;)</expression>
    </formulas>
